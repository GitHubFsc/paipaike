.studentInfo .classTitle {
    color: #1a1a1a;
    margin-right: 25rpx;
}

.rightBtn {
    width: 45%;
    height: 70rpx;
    margin:  0 auto;
    
}
.rightBtn view{
    width: 100%;
    height: 100%;
    background-size: 100%;
    border: 4rpx solid #469bba;
    border-radius: 20rpx;
    position: relative;
    margin: 60rpx auto;
    background-color: #469bba;
}
.rightBtn image {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    background-size: 100% 100%;
    top: 20rpx;
    left: 60rpx;
}

.rightBtn .rightTxt {
    color: #fff !important;
    text-align: center;
    line-height: 70rpx;
    display: block;
    font-size: 30rpx;
}

.rightBtn text {
    color: #469bba;
    text-align: center;
    line-height: 70rpx;
    display: block;
    font-size: 30rpx;
}
