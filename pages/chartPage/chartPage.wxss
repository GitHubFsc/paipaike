 .table .thead {
    height: 101rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-top: 20rpx;
    background-color: #479AB9;
    color: #fff;
    font-size: 26rpx;
}
 .table .thead .td {
    height: 100%;
    display: flex;
    flex: 1;
    justify-content: center;
    align-items: center;
    border-right: 1rpx solid #D8D8D8;
}
 .table .tr {
    height: 100rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #333;
    font-size: 28rpx;
    border-bottom: 1rpx solid #D8D8D8;
}
 .table .tr .td {
    height: 100%;
    display: flex;
    flex: 1;
    justify-content: center;
    align-items: center;
    border-right: 1rpx solid #D8D8D8;
}
 .table .odd {
    background-color: #fff;
}
 .even {
    background-color: #F6F7FA;
}
.table-title {
    margin: 57rpx 0 31rpx 27rpx;
    color: #147092;
    font-size: 32rpx;
}