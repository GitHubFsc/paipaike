/* ========================================= */

.search {
    width: 70%;
    height: 60rpx;
    background-size: 28rpx;
    border-radius: 15rpx;
    margin: 20rpx auto 40rpx auto;
    border: 1rpx solid #bcbcbc;
    margin-left: 15%;
}

.search input {
    width: 90%;
    height: 100%;
    float: right;
    font-size: 27rpx;
}

.rightAdd {
    border: 1rpx solid #479aba;
    border-radius: 10rpx;
    height: 58rpx;
    width: 75rpx;
    margin-top: 20rpx;
    margin-left: 3%;
}

.rightAdd image {
    width: 45rpx;
    height: 45rpx;
    background-size: 100% 100%;
    margin-top: 7rpx;
    margin-left: 16rpx;
}

.studentItem {
    width: 100%;
    overflow: hidden;
    border-radius: 6rpx;
    height: 80rpx;
    margin-bottom: 10rpx;
    line-height: 80rpx;
    position: relative;
    background-color: #fff;
    box-shadow: 0 0 10rpx rgba(153, 153, 153, 0.2);
}

.studentItem .studentInfo {
    width: 100%;
    font-size: 28rpx;
}

.studentInfo .ite {
    padding-left: 5%;
    padding-right: 5%;
    line-height: 80rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    width: 82%;
    display: inline-block;
    height: 80rpx;
    vertical-align: middle;
}

.select-icon {
    width: 8%;
    display: inline-block;
    height: 80rpx;
    vertical-align: middle;
}

.select-icon image {
    position: absolute;
    width: 35rpx;
    height: 35rpx;
    background-size: 100% 100%;
    margin-top: 23rpx;
}

.studentInfo .classTitle {
    color: #1a1a1a;
    margin-right: 25rpx;
}

.rightBtn {
    width: 45%;
    height: 70rpx;
    background-size: 100%;
    margin-top: 30rpx;
    border: 4rpx solid #469bba;
    border-radius: 20rpx;
    position: relative;
    margin: 60rpx auto 0 auto;
    background-color: #469bba;
}

.rightBtn image {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    background-size: 100% 100%;
    top: 20rpx;
    left: 60rpx;
}

.rightBtn .rightTxt {
    color: #fff !important;
    text-align: center;
    line-height: 70rpx;
    display: block;
    font-size: 30rpx;
}

.rightBtn text {
    color: #469bba;
    text-align: center;
    line-height: 70rpx;
    display: block;
    font-size: 30rpx;
}

