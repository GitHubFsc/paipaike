    position: relative;
    margin-top: 10rpx;
}

.uploadImg image {
    position: absolute;
    width: 80rpx;
    height: 80rpx;
    background-size: 100% 100%;
    top: 50%;
    margin-top: -38rpx;
    left: 50%;
    margin-left: -30rpx;
}

.uploadedImg {
    margin-top: 20rpx;
    overflow: hidden;
}

.imgItem {
    width: 25%;
    height: 140rpx;
