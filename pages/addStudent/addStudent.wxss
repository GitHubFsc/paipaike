page {
    background-color: #fff;
}

.mainTitle {
    height: 80rpx;
    font-size: 34rpx;
    text-indent: 40rpx;
    box-shadow: 0 0 10rpx rgba(153, 153, 153, 0.2);
    border-bottom: 4rpx solid #469db9;
    line-height: 80rpx;
    margin-bottom: 50rpx;
    color: #469db9;
}

.item {
    line-height: 100rpx;
    height: 100rpx;
}

.item .itemTitle {
    width: 23%;
    float: left;
    font-size: 30rpx;
    text-align: center;
}

.item .itemContent {
    width: 77%;
    float: right;
}

.item input {
    height: 70rpx;
    margin-top: 15rpx;
    font-size: 28rpx;
    border: 1px solid #efefef;
    border-radius: 8rpx;
    color: #999;
    padding: 0 20rpx;
}

.keshiInput {
    height: 100rpx;
}

.keshiInput input {
    width: 80%;
    display: inline-block;
}

.keshiInput text {
    display: inline-block;
    vertical-align: top;
    padding-left: 15rpx;
    font-size: 30rpx;
    color: #999;
}

.select_box {
    background: #fff;
    width: 100%;
    margin: 15rpx auto;
    position: relative;
}

.select {
    box-sizing: border-box;
    width: 100%;
    height: 70rpx;
    border: 1px solid #efefef;
    border-radius: 8rpx;
    display: flex;
    align-items: center;
    padding: 0 20rpx;
}

.select_text {
    font-size: 26rpx;
    flex: 1;
    color: #999;
}

.select_img {
    width: 30rpx;
    height: 30rpx;
    display: block;
    transition: transform 0.3s;
    background-size: 100% 100%;
}

.select_img_rotate {
    transform: rotate(90deg);
}

.option_box {
    position: absolute;
    top: 70rpx;
    width: 100%;
    border: 1px solid #efefef;
    box-sizing: border-box;
    overflow-y: auto;
    border-top: 0;
    background: #fff;
    transition: height 0.3s;
    z-index: 2;
    height: 350rpx;
    color: #999;
}

.option {
    display: block;
    line-height: 40rpx;
    font-size: 26rpx;
    border-bottom: 1px solid #efefef;
    padding: 10rpx;
}

.lastItem,
.lastRight {
    width: 50%;
    display: inline-block;
    font-size: 30rpx;
}

.lastRight {
    color: #999;
}

.checked-lable radio {
    transform: scale(0.8);
}

.leftLabel {
    margin-right: 40rpx;
}

.bgBlue {
    background-color: #479aba;
}

.bgBlue text {
    color: #fff;
}



