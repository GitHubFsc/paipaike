page {
    width: 100%;
    height: 100%;
    background-color: #fff;
}

.nav {
    width: 100%;
    box-sizing: border-box;
    background: #469bba;
    height: 78rpx;
}

.nav-item {
    display: inline-block;
    text-align: center;
    font-size: 30rpx;
    height: 70rpx;
    line-height: 70rpx;
    width: 50%;
    color: #fff;
}

.line {
    width: 60rpx;
    height: 4rpx;
    background-color: #fff;
    text-align: center;
    margin: 0 auto;
}

.nav-item.active {
    color: #fff;
}

.tab-box {
    width: 100%;
    height: 100%;
    margin-top: 0rpx;
    border-top: none;
    border-radius: 0rpx 0rpx 13rpx 13rpx;
}

.tab-content {
    overflow-y: scroll;
}

.swiperItems image {
    width: 100%;
}


/* =========请假信息============= */

.dateInfo {
    margin-top: 20rpx;
    padding-bottom: 40rpx;
}

.selectTitle {
    display: inline-block;
}

.selectTitle image {
    width: 130rpx;
    height: 45rpx;
    background-size: 100% 100%;
    margin-left: 20rpx;
    vertical-align: middle;
}

.dateTime {
    padding: 30rpx 0;
    font-size: 30rpx;
    font-weight: lighter;
    color: #000;
    display: inline-block;
    vertical-align: middle;
    margin-left: 20rpx;
}

