/* *app.wxss*/

page {
    background-color: #f5f5f5;
}

.container {
    width: 100%;
}

.container-cn {
    width: 710rpx;
    margin: 0 auto;
}

radio {
    transform: scale(0.8);
}

button,
input {
    border: none;
    padding: 0;
    margin: 0;
}

.left {
    float: left;
}

.right {
    float: right;
}

.clear {
    clear: both;
}

.h100 {
    width: 100%;
    height: 100rpx;
}

.color9 {
    color: #999;
}

.button {
    width: 60%;
    height: 65rpx;
    background-size: 100%;
    margin-top: 30rpx;
    border: 4rpx solid #469bba;
    border-radius: 8rpx;
    position: relative;
    margin: 60rpx auto 0 auto;
}

.curButton {
    width: 60%;
    height: auto;
    padding: 30rpx 0 60rpx 0;
    border-radius: 8rpx;
    margin: 0rpx auto;
    text-align: center;
}

.curButton text {
    display: inline-block;
    width: 60%;
    height: 65rpx;
    border-radius: 8rpx;
    margin: 40rpx auto;
    background-color: #469bba;
    text-align: center;
    line-height: 65rpx;
    font-size: 26rpx;
    color: #fff;
}

.picker {
    height: 70rpx;
    position: relative;
    text-align: center;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: left;
}

.curButton text {
    color: #fff;
    line-height: 60rpx;
    display: inline-block;
    vertical-align: middle;
    font-size: 30rpx;
}

.button text {
    color: #469bba;
    text-align: center;
    line-height: 65rpx;
    display: block;
    font-size: 30rpx;
}

.button .icon {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    background-size: 100% 100%;
    top: 20rpx;
    right: 30%;
}

.hei80r {
    height: 80rpx;
}

/**app.wxss**/

.commodity_screen {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0, 0, 0, 0.5);
    overflow: hidden;
    z-index: 9;
    color: #fff;
}

.commodity_attr_box {
    width: 80%;
    position: fixed;
    top: 40%;
    left: 0;
    z-index: 10;
    background: #fff;
    padding-top: 20rpx;
    margin-left: 10%;
    margin-top: -150rpx;
    border-radius: 15rpx;
    padding-bottom: 30rpx;
}

.commodity_attr_box .title {
    position: relative;
    width: 100%;
    text-align: center;
    height: 80rpx;
    line-height: 80rpx;

}

.commodity_attr_box .linkItem .title {
    position: relative;
    width: 380rpx;
    height: 80rpx;
    line-height: 80rpx;
    padding-left: 20rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    text-align: left;
}

.commodity_attr_box .title text:first-child {
    color: #469bba;
    font-size: 36rpx;
    text-align: center;
    margin-top: 50rpx;
    display: block;
}

.linkItem {
    width: 85%;
    height: 80rpx;
    background-color: #fff;
    box-shadow: 0 0 10rpx rgba(153, 153, 153, 0.2);
    border-radius: 10rpx;
    margin-bottom: 30rpx;
    margin: 30rpx auto 30rpx auto;
    position: relative;
    display: flex;
    border: 1rpx solid rgba(153, 153, 153, 0.2);
}

.linkItem .leftImg image {
    width: 51rpx;
    background-size: 100%;
    margin-top: 16rpx;
    margin-left: 50rpx;
    margin-right: 30rpx;
}

.linkItem .txt {
    line-height: 70rpx;
    text-align: left;
    font-size: 26rpx;
    position: relative;
    height: 70rpx;
    width: 100%;
}

.txt input {
    width: 85%;
    height: 76rpx;
    position: absolute;
    margin-top: 0rpx;
}

.dialogBtn {
    width: 90%;
    margin: 0 auto;
    padding: 5rpx;
}

.buttonDia {
    width: 100%;
    height: 80rpx;
    background-color: #d5bf8f;
    border-radius: 8rpx;
    color: #fff;
    font-size: 28rpx;
    line-height: 80rpx;
    text-align: center;
}

